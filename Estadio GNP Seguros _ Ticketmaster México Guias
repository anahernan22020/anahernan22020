.carousel .slick-slide { height: inherit !important; }

.carousel .slick-slide > div { height: 100%; }

.carousel .slick-slide > div > div { height: 100%; }

.carousel article { display: flex; width: 100%; align-items: center; padding: 0px 10px 10px; height: 100%; position: relative; }

.carousel article .article-content { display: flex; flex-flow: column wrap; justify-content: flex-start; border-radius: 2px; background: rgb(255, 255, 255); border: 1px solid rgb(235, 235, 235); box-shadow: rgba(0, 0, 0, 0.12) 0px 4px 4px 0px; height: 100%; }

