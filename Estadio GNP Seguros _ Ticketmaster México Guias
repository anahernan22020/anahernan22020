.carousel article .article-content p.cta { padding-bottom: 0px; font-size: 18px; line-height: 22px; font-weight: 700; margin-top: auto; margin-bottom: 0px; }

.carousel article .article-content p.cta a { width: 100%; text-decoration: none; }

.carousel article .article-content p.cta a::after { position: absolute; inset: 0px; content: ""; display: block; z-index: 10; }

.carousel article .article-content p.cta a svg { fill: rgb(2, 77, 223); display: inline; vertical-align: top; transform: rotate(-90deg); }

.carousel article .article-content p.cta a:focus, .carousel article .article-content p.cta a:hover { text-decoration: underline; }

.carousel article .article-content p.cta a:focus svg, .carousel article .article-content p.cta a:hover svg { fill: rgb(2, 62, 178); }

.carousel article .text-slide { display: flex; width: 100%; height: 100%; flex-flow: column wrap; justify-content: center; padding: 25px; border: 1px solid rgb(235, 235, 235); }

.carousel article .text-slide h3 { text-transform: none; }

.carousel article .text-slide p { color: rgb(100, 100, 100); }

